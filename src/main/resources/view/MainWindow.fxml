<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="400.0" prefWidth="1249.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <stylesheets>
            <URL value="@DarkTheme.css" />
            <URL value="@Extensions.css" />
         </stylesheets>
         <children>
            <MenuBar fx:id="menuBar" VBox.vgrow="NEVER">
               <menus>
                  <Menu mnemonicParsing="false" text="File">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Help">
                     <items>
                        <MenuItem fx:id="helpMenuItem" mnemonicParsing="false" onAction="#handleHelp" text="Help" />
                        <MenuItem fx:id="helpOverlayItem" mnemonicParsing="false" onAction="#handleOverlay" text="Overlay" visible="false" />
                        <MenuItem fx:id="helpOverlayExit" mnemonicParsing="false" onAction="#handleOverlayExit" text="OverlayExit" visible="false" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
            <StackPane fx:id="commandBoxPlaceholder" styleClass="pane-with-border" VBox.vgrow="NEVER">
               <padding>
                  <Insets bottom="5" left="10" right="10" top="5" />
               </padding>
            </StackPane>
            <StackPane fx:id="resultDisplayPlaceholder" maxHeight="155.0" minHeight="78.0" prefHeight="78.0" prefWidth="690.0" styleClass="pane-with-border" VBox.vgrow="NEVER">
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </StackPane>
            <HBox fx:id="sortFindPanelPlaceholder" prefHeight="44.0" prefWidth="690.0" styleClass="pane-with-border" />
            <SplitPane id="splitPane" fx:id="splitPane" dividerPositions="0.4860335195530726, 0.5251396648044693" VBox.vgrow="ALWAYS">
               <items>
                  <VBox fx:id="personList" style="-fx-border-color: white; -fx-border-width: 1.70;" SplitPane.resizableWithParent="false">
                     <padding>
                        <Insets bottom="10" left="10" right="10" top="10" />
                     </padding>
                     <children>
                        <StackPane fx:id="personListPanelPlaceholder" minWidth="300.0" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
                  <VBox fx:id="pinList" spacing="0.5" style="-fx-border-color: white; -fx-border-width: 1.7;" SplitPane.resizableWithParent="false">
                     <children>
                        <Label style="-fx-text-fill: white;" text="Pinned Person">
                           <VBox.margin>
                              <Insets left="110.0" />
                           </VBox.margin>
                        </Label>
                        <StackPane fx:id="pinListPlaceHolder" minWidth="300.0" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
                  <StackPane fx:id="browserPlaceholder" minWidth="600.0" style="-fx-border-color: white; -fx-border-width: 1.70;" />
               </items>
            </SplitPane>
            <StackPane fx:id="statusbarPlaceholder" VBox.vgrow="NEVER" />
         </children>
      </VBox>
      <StackPane>
         <children>
            <ImageView fx:id="helpOverlay" disable="true" fitHeight="403.0" fitWidth="1250.0" pickOnBounds="true" preserveRatio="true" visible="false">
               <image>
                  <Image url="@../../../../docs/images/Architecture.png" />
               </image>
            </ImageView>
         </children>
      </StackPane>
   </children>
</StackPane>
